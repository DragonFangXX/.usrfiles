#!/bin/bash

exec >> ~/documents/logs/autostart
exec 2>&1

echo "$(date)=============================="
compton -b
st &							#URxvt Terminal
keepass &						#Keepass Password Manager

conky -c ~/.config/conky/conf/weather.conf &
conky -c ~/.config/conky/conf/clock.conf &
conky -c ~/.config/conky/conf/disk.conf &
conky -c ~/.config/conky/conf/system.conf &
conky -c ~/.config/conky/conf/fs.conf &
conky -c ~/.config/conky/conf/battery.conf &
conky -c ~/.config/conky/conf/cpu.conf &
conky -c ~/.config/conky/conf/network.conf &
conky -c ~/.config/conky/conf/memory.conf &

/usr/local/bin/hdmi_toggle
#i3lock -B=10 --verifsize=32 --wrongsize=32 --force-clock --bar-indicator --timesize=128 --keylayout 0 --ringwrongcolor=ff0000ff --layoutcolor=ffffffff --timecolor=ffffffff --datecolor=ffffffff --verifcolor=ffffffff --wrongcolor=ffffffff --ringvercolor=ffff00ff --timepos=300:950 --datepos=200:1025 --datesize=32 --layoutsize=32 --layoutpos=1750:1025
